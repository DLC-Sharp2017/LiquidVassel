<Window x:Class="LiquidVassel.AjouteruneTourner"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LiquidVassel"
        mc:Ignorable="d"
        Title="AjouteruneTourner" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="idtourner" HorizontalAlignment="Left" Height="30" Margin="35,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="215"/>
        <TextBox x:Name="nomTourner" HorizontalAlignment="Left" Height="35" Margin="40,145,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="210"/>
        <Label x:Name="idtourne" Content="ID Tourné" HorizontalAlignment="Left" Margin="35,34,0,0" VerticalAlignment="Top"/>
        <Label x:Name="nomtournee" Content="Nom de la Tourné" HorizontalAlignment="Left" Margin="40,119,0,0" VerticalAlignment="Top"/>
        <Button x:Name="enregistrer" Content="Save" HorizontalAlignment="Left" Height="20" Margin="85,205,0,0" VerticalAlignment="Top" Width="125" Click="enregistrer_Click"/>
    </Grid>
</Window>
